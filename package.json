{
  "author": "Girma Nigusse <xgirma@gmail.com>",
  "bugs": {
    "url": "https://github.com/e2e-boilerplate/utils/issues"
  },
  "dependencies": {
    "compare-versions": "3.6.0",
    "js-yaml": "3.13.1",
    "markdown-table": "2.0.0",
    "minimist": "1.2.5",
    "moment": "2.25.3",
    "pino": "6.2.1",
    "pino-pretty": "4.0.0",
    "rimraf": "3.0.2"
  },
  "description": "Utils end-to-end test automation boilerplate. ",
  "devDependencies": {
    "eslint": "7.0.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-prettier": "3.1.3",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5"
  },
  "homepage": "https://github.com/e2e-boilerplate/utils#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "keywords": [
    "boilerplate",
    "e2e",
    "e2e tests",
    "example",
    "integration test",
    "utils"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "name": "utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplate/utils.git"
  },
  "scripts": {
    "repos:meta:get": "node -r esm exec/repos_meta.js --pages 3",
    "git:clone": "node -r esm exec/runner.js --task clone",
    "git:pull": "node -r esm exec/runner.js --task pull",
    "dependencies:update": "node -r esm exec/runner.js --task dependencies",
    "package:json:write": "node -r esm exec/runner.js --task metadata",
    "git:add": "node -r esm exec/runner.js --task add",
    "git:commit": "node -r esm exec/runner.js --task commit --message updates",
    "git:push": "node -r esm exec/runner.js --task push",
    "set:funding": "node -r esm exec/runner.js --task funding",
    "set:readme": "node -r esm exec/runner.js --task readme",
    "set:workflow": "node -r esm exec/runner.js --task workflow -module wd",
    "style:eslintrc": "node -r esm exec/runner.js --task eslintrc",
    "style:tslint": "node -r esm exec/runner.js --task tslint",
    "config:jest": "node -r esm exec/runner.js --task jest",
    "config:mocha": "node -r esm exec/runner.js --task mocha",
    "config:protractor": "node -r esm exec/runner.js --task protractor --module protractor",
    "config:webdriverio": "node -r esm exec/runner.js --task webdriverio --module webdriverio",
    "tsconfig": "node -r esm exec/runner.js --task tsconfig",
    "gist:create": "node -r esm exec/gist_create.js --token ...",
    "gist:get": "node -r esm exec/gist_get.js --username xgirma",
    "gist:list": "node -r esm exec/gist_list.js",
    "gist:update": "node -r esm exec/gist_update.js --token ...",
    "npm:audit": "node -r esm exec/runner.js --task audit",
    "npm:install:deps": "node -r esm exec/runner.js --task install",
    "npm:lint": "npx eslint --ext .js ./ --fix",
    "npm:lint:repos": "node -r esm runner.js --task lint",
    "matrix:all": "node -r esm exec/matrix.js",
    "matrix:implemented": "node -r esm exec/matrix_implementend.js",
    "repos:list:get": "node -r esm exec/repos_list.js --pages 2",
    "traffic:meta:get": "node -r esm exec/runner.js --task traffic --token ...",
    "traffic:doc:write": "node -r esm exec/traffic.js --task trafic",
    "gen:cron": "node -r esm runner.js --task cron",
    "any:execute": "node -r esm exec/runner.js --task command"
  },
  "version": "1.0.1"
}
