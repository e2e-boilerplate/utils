{
  "author": "Girma Nigusse <xgirma@gmail.com>",
  "bugs": {
    "url": "https://github.com/e2e-boilerplate/utils/issues"
  },
  "dependencies": {
    "compare-versions": "3.6.0",
    "js-yaml": "3.13.1",
    "markdown-table": "2.0.0",
    "minimist": "1.2.5",
    "moment": "2.25.1",
    "pino": "6.2.1",
    "pino-pretty": "4.0.0",
    "rimraf": "3.0.2"
  },
  "description": "Utils end-to-end test automation boilerplate. ",
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-cypress": "2.10.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-prettier": "3.1.3",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5"
  },
  "homepage": "https://github.com/e2e-boilerplate/utils#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "keywords": [
    "boilerplate",
    "e2e",
    "e2e tests",
    "example",
    "integration test",
    "utils"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "name": "utils",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplate/utils.git"
  },
  "scripts": {
    "any:execute": "node -r esm exec/runner.js --task command --username e2e-boilerplate",
    "config:jest": "node -r esm exec/runner.js --task jest --username e2e-boilerplate",
    "config:mocha": "node -r esm exec/runner.js --task mocha --username e2e-boilerplate",
    "config:protractor": "node -r esm exec/runner.js --task protractor --username e2e-boilerplate --module protractor",
    "config:webdriverio": "node -r esm exec/runner.js --task webdriverio --username e2e-boilerplate --module webdriverio",
    "dependencies": "node -r esm runner.js --task dependencies --username e2e-boilerplate",
    "eslintrc": "node -r esm runner.js --task eslintrc --username e2e-boilerplate",
    "gen:cron": "node -r esm runner.js --task cron --username e2e-boilerplate",
    "get:repos": "node -r esm exec/repositories.js --pages 2 --username e2e-boilerplate",
    "gist:create": "node -r esm exec/gist_create.js --username e2e-boilerplate --token token",
    "gist:get": "node -r esm exec/gist_get.js --username xgirma",
    "gist:list": "node -r esm exec/gist_list.js",
    "gist:update": "node -r esm exec/gist_update.js --username e2e-boilerplate --token token",
    "git:add": "node -r esm runner.js --task add --username e2e-boilerplate",
    "git:clone": "node -r esm runner.js --task clone --username e2e-boilerplate",
    "git:commit": "node -r esm runner.js --task commit --username e2e-boilerplate --message updates",
    "git:pull": "node -r esm runner.js --task pull --username e2e-boilerplate",
    "git:push": "node -r esm runner.js --task push --username e2e-boilerplate",
    "matrix": "node -r esm exec/matrix.js --username e2e-boilerplate",
    "matrix:implemented": "node -r esm exec/matrix_implementend.js --username e2e-boilerplate",
    "npm:audi": "node -r esm runner.js --task audit --username e2e-boilerplate",
    "npm:install:deps": "node -r esm runner.js --task install --username e2e-boilerplate",
    "npm:lint": "npx eslint --ext .js ./ --fix",
    "npm:lint:repos": "node -r esm runner.js --task lint --username e2e-boilerplate",
    "set:funding": "node -r esm runner.js --task funding --username e2e-boilerplate",
    "set:metadata": "node -r esm runner.js --task metadata --username e2e-boilerplate --keywords \"e2e-e2e tests-boilerplate-example-integration test\" --author \"Girma Nigusse <xgirma@gmail.com>\"",
    "set:readme": "node -r esm runner.js --task readme --username e2e-boilerplate",
    "set:workflow": "node -r esm runner.js --task workflow --username e2e-boilerplate -module wd",
    "tsconfig": "node -r esm runner.js --task tsconfig --username e2e-boilerplate",
    "tslint": "node -r esm runner.js --task tslint --username e2e-boilerplate"
  },
  "version": "1.0.1"
}